<UserControl x:Class="UserManagementApplication.UserInterface.InputEmployee"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UserManagementApplication.UserInterface"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid Background="#FF08A3D1" Margin="0,-19,-297,-326" RenderTransformOrigin="0.543,0.507">
            <Label x:Name="label" Content="Input Employee" HorizontalAlignment="Left" Margin="238,0,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Black" FontSize="18"/>
